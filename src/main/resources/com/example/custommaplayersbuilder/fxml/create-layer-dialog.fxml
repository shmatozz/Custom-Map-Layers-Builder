    <Button fx:id="pathSelect" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" onAction="#selectSavePath" prefHeight="40.0" prefWidth="200.0" stylesheets="@../styles/fxbuttons.css" text="Где сохранить?">
