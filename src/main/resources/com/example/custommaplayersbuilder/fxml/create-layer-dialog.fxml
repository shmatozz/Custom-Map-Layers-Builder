      </VBox.margin></Button>
    <Button fx:id="convertPolygonButton" disable="true" maxHeight="-Infinity" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" onAction="#convertPolygon" prefHeight="35.0" prefWidth="180.0" stylesheets="@../styles/fxbuttons.css" text="Только полигон">
