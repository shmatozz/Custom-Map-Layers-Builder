      <VBox.margin>
         <Insets left="30.0" right="30.0" />
      </VBox.margin></Button>
    <Label fx:id="logText" minHeight="0.0" minWidth="0.0" VBox.vgrow="ALWAYS">
        <font>
            <Font name="Inter Regular" size="14.0"/>
        </font>
        <style>-fx-font-weight: bold;</style>
    </Label>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
