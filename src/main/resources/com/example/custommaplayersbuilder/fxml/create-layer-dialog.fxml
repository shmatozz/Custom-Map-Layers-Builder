         <Separator HBox.hgrow="ALWAYS" />
         <Label text="Затем">
            <font>
               <Font name="Inter Regular" size="14.0" />
            </font>
         </Label>
         <Separator HBox.hgrow="ALWAYS" />
      </children>
   </HBox>
   <Label text="Выберите объект для конвертации">
      <font>
         <Font name="Inter Regular" size="16.0" />
